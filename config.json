{
  "log": {
    "level": "warn",
    "timestamp": true
  },
  "dns": {
    "servers": [
      {
        "tag": "dns_direct",
        "address": "https://223.5.5.5/dns-query",
        "detour": "direct"
      },
      {
        "tag": "dns_proxy",
        "address": "https://8.8.8.8/dns-query",
        "detour": "proxy"
      },
      {
        "tag": "dns_block",
        "address": "rcode://refused"
      },
      {
        "tag": "dns_fakeip",
        "address": "fakeip"
      }
    ],
    "rules": [
      {
        "outbound": "any",
        "server": "dns_direct"
      },
      {
        "rule_set": [
          "geosite-category-ads-all",
          "BlockHttpDNS"
        ],
        "server": "dns_block"
      },
      {
        "rule_set": "geosite-geolocation-!cn",
        "query_type": [
            "A",
            "AAAA"
        ],
        "server": "dns_fakeip"
      },
      {
        "rule_set": "geosite-geolocation-!cn",
        "server": "dns_proxy"
      },
      {
        "rule_set": [
          "geosite-category-companies@cn",
          "geosite-cn"
        ],
        "server": "dns_direct"
      }
    ],
    "final": "dns_proxy",
    "strategy": "ipv4_only",
    "disable_cache": false,
    "disable_expire": false,
    "fakeip": {
        "enabled": true,
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
    },
    "independent_cache": true
  },
  "inbounds": [
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "block",
      "tag": "block"
    },
    {
      "type": "dns",
      "tag": "dns-out"
    }
  ],
  "route": {
    "rules": [
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "protocol": "dns"
          },
          {
            "port": 53
          }
        ],
        "outbound": "dns-out"
      },
      {
        "ip_is_private": true,
        "outbound": "direct"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "network": "udp",
            "port": 443
          },
          {
            "rule_set": [
              "geoip-cn",
              "geosite-cn"
            ],
            "invert": true
          }
        ],
        "outbound": "block"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "port": 853
          },
          {
            "protocol": "stun"
          },
          {
            "protocol": "quic"
          },
          {
            "rule_set": [
              "geosite-category-ads-all",
              "BlockHttpDNS"
            ]
          }
        ],
        "outbound": "block"
      },
      {
        "rule_set": [
            "geoip-cn",
            "geosite-geolocation-cn",
            "geosite-apple@cn",
            "geosite-icloud@cn"
        ],
        "outbound": "direct"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
            {
                "rule_set": "geoip-cn"
            },
            {
                "rule_set": [
                    "geosite-cn",
                    "geosite-category-companies@cn"
                ]
            }
        ],
        "outbound": "direct"
      },
      { 
        "type": "logical",
        "mode": "or",
        "rules": [
            {
                "rule_set": [
                  "geosite-google",
                  "geosite-openai",
                  "geosite-geolocation-!cn"
                ]
            },
            {
                "domain_suffix": "googleapis.cn"
            }
        ],
        "outbound": "proxy"
      }
    ],
    "final": "proxy",
    "auto_detect_interface": true,
    "rule_set": [
      {
        "tag": "geoip-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/lyc8503/sing-box-rules/rule-set-geoip/geoip-cn.srs",
        "download_detour": "proxy"
      },
      {
        "tag": "geosite-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/lyc8503/sing-box-rules/rule-set-geosite/geosite-cn.srs",
        "download_detour": "proxy"
      },
      {
        "tag": "geosite-geolocation-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/lyc8503/sing-box-rules/rule-set-geosite/geosite-geolocation-cn.srs",
        "download_detour": "proxy"
      },
      {
        "tag": "geosite-geolocation-!cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/lyc8503/sing-box-rules/rule-set-geosite/geosite-geolocation-!cn.srs",
        "download_detour": "proxy"
      },
      {
        "tag": "geosite-category-companies@cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/lyc8503/sing-box-rules/rule-set-geosite/category-companies@cn.srs",
        "download_detour": "proxy"
      },
      {
        "tag": "geosite-category-ads-all",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/lyc8503/sing-box-rules/rule-set-geosite/category-ads-all.srs",
        "download_detour": "proxy"
      },
      {
        "tag": "geosite-google",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/lyc8503/sing-box-rules/rule-set-geosite/geosite-google.srs",
        "download_detour": "proxy"
      },
      {
        "tag": "geosite-icloud@cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/lyc8503/sing-box-rules/rule-set-geosite/geosite-icloud@cn.srs",
        "download_detour": "proxy"
      },
      {
        "tag": "geosite-apple@cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/lyc8503/sing-box-rules/rule-set-geosite/geosite-apple@cn.srs",
        "download_detour": "proxy"
      },
      {
        "tag": "geosite-openai",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/lyc8503/sing-box-rules/rule-set-geosite/geosite-openai.srs",
        "download_detour": "proxy"
      },
      {
        "tag": "BlockHttpDNS",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/bm7/BlockHttpDNS.srs",
        "download_detour": "proxy"
      }
    ]
  },
  "experimental": {
    "clash_api": {
      "external_controller": "127.0.0.1:9999",
      "external_ui_download_url": "",
      "secret": "",
      "default_mode": "",
      "external_ui_download_detour": "",
      "external_ui": "ui"
    },
    "cache_file": {
      "enabled": true,
      "path": "cache.db",
      "cache_id": "sing-box_cache",
      "store_fakeip": true
    }
  }
}
